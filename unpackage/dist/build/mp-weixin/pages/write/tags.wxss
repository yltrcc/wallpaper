.tag{font-size:14px;background-color:#f5f6f8;color:#909399;border-radius:40rpx;line-height:52rpx;padding:0 42rpx;text-align:center;margin-right:20rpx;margin-bottom:20rpx}.btn{font-size:16px;background-image:linear-gradient(100deg,#fcfc44,#f8c93c);color:#333;border-radius:40rpx;line-height:86rpx;text-align:center;width:100%}.tag-active{background-image:linear-gradient(100deg,#fcfc44,#f8c93c);color:#333;box-shadow:0 0 10px #fff8ab}.bottom{width:100vw;position:fixed;bottom:0;box-shadow:0 0 16px #d8d8d8;border-radius:24rpx 24rpx 0 0;background-color:#fff}.hover-class{-webkit-transform:scale(.92);transform:scale(.92);transition:all .2s}.u-size-default{padding:15rpx 30rpx!important}